{
  "version": 2,
  "alias": "pavelgonzales.com",
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder@0.17.1",
      "config": {}
    }
  ],
  "routes": [
    { "src": "/_nuxt/.+", "headers": { "Cache-Control": "max-age=31557600" } },
    { "src": "/(.*)", "dest": "/" }
  ]
}